        BorderColor3 = Color3.fromRGB(0, 0, 0),
        BorderSizePixel = 0,
        Position = UDim2.new(0, 574, 0, 10),
        Size = UDim2.new(0, 30, 0, 20),
    })

    Library:AddStroke(Close, Border, Brown)

    Library:AddGradient(Close, Color3.fromRGB(170,0,0), Color3.fromRGB(255,100,100))

    local Close_Button = Library:Create('TextButton', {
        Parent = Close,
        BackgroundColor3 = Color3.fromRGB(255, 255, 255),
        BackgroundTransparency = 1.000,
        BorderColor3 = Color3.fromRGB(0, 0, 0),
        BorderSizePixel = 0,
        Rotation = 45.000,
        Size = UDim2.new(1, 0, 1, 0),
        AutoButtonColor = false,
        Font = Enum.Font.SourceSansBold,
        Text = "+",
        Visible = true,
        TextColor3 = Color3.fromRGB(255, 255, 255),
        TextSize = 25.000,
    })

    Close_Button.MouseEnter:Connect(function()
        Utility:Tween(Close_Button, {Rotation = 315}, .15)
    end)

    Close_Button.MouseLeave:Connect(function()
        Utility:Tween(Close_Button, {Rotation = 45}, .15)
    end)

    Close_Button.MouseButton1Down:Connect(function()
        Utility:Tween(Close_Button, {Rotation = 45}, .15)
    end)

    Close_Button.MouseButton1Up:Connect(function()
        Utility:Tween(Close_Button, {Rotation = 315}, .15)
    end)

    Close_Button.MouseButton1Click:Connect(function()
        Utility:Tween(Main_Body, {Size = UDim2.new(0, 0, 0, 0)}, .15)
        wait(.3)
        Main_Body.Visible = false
    end)

    local Setting_Frame = Library:Create('Frame', {
        Parent = Main_Body,
        AnchorPoint = Vector2.new(0.5, 0.5),
        BackgroundColor3 = Color3.fromRGB(255, 255, 255),
        BackgroundTransparency = 0.300,
        BorderColor3 = Color3.fromRGB(0, 0, 0),
        BorderSizePixel = 0,
        Position = UDim2.new(0, 543, 0, 10),
        Size = UDim2.new(0, 30, 0, 20),
    })

    Library:AddStroke(Setting_Frame, Border, Brown)

    Library:AddGradient(Setting_Frame, White, Color3.fromRGB(200,200,200))

    local Setting_Button = Library:Create('ImageButton', {
        Parent = Setting_Frame,
        BackgroundColor3 = Color3.fromRGB(255, 255, 255),
        BackgroundTransparency = 1.000,
        BorderColor3 = Color3.fromRGB(0, 0, 0),
        BorderSizePixel = 0,
