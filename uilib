function Library:SetUIScale(val)
    UIScale.Scale = val
end

function Library:GetUIScale()
    return UIScale.Scale
end

function Library:DestroyGUI()
    for i,v in pairs(CoreGui:GetChildren()) do
        if v:IsA("ScreenGui") and (v.Name == GUI_Name or v.Name == 'NotificationsBT') then
            v:Destroy();
        end;
    end;
end;

function Library:ToggleUI(isKeyCodes)
    for i,v in pairs(CoreGui:GetChildren()) do
        if v:IsA("ScreenGui") and v.Name == GUI_Name then
            game:GetService("UserInputService").InputBegan:connect(function(input) 
                if input.KeyCode == isKeyCodes then
                    v.Enabled = not v.Enabled
                end
            end)
        end;
    end;
end;

function Library:Create(Class, Properties)
    local _Instance = Class;

    if type(Class) == 'string' then
        _Instance = Instance.new(Class);
    end;

    for Property, Value in next, Properties do
        _Instance[Property] = Value;
    end;

    return _Instance;
end;

function Library:AttemptSave()
    if Library.SaveManager then
        Library.SaveManager:Save();
    end;
end;

local Contextual = Enum.ApplyStrokeMode.Contextual
local Border = Enum.ApplyStrokeMode.Border

function Library:AddStroke(isParent, isType, isColor)
    Library:Create('UIStroke', {
        Parent = isParent,
        ApplyStrokeMode = isType,
        Color = isColor
    })
end

function Library:AddGradient(isParent, isColor1, isColor2)
    Library:Create('UIGradient', {
        Parent = isParent,
        Color = ColorSequence.new({
            ColorSequenceKeypoint.new(0, isColor1),
            ColorSequenceKeypoint.new(0.3, isColor1),
            ColorSequenceKeypoint.new(1, isColor2),
        })
    })
end

function Library:GetTextBounds(Text, Font, Size, Resolution)
    local Bounds = TextService:GetTextSize(Text, Size, Font, Resolution or Vector2.new(1920, 1080))
    return Bounds.X, Bounds.Y
end;

local TooltipFolder = Instance.new("Folder", BLACKTRAP)
TooltipFolder.Name = "Tooltips"

function Library:AddToolTip(InfoString, TargetParent)
    local X, Y = Library:GetTextBounds(InfoString, Library.Font, 12);

    local Tooltip = Library:Create('Frame', {
        BackgroundColor3 = Library.MainColor,
        BorderColor3 = Library.MainColor,
        BorderSizePixel = 0,
        Position = TargetParent.Position,
        Size = UDim2.new(0, X + 5, 0, Y + 4),
        ZIndex = 100,
        Parent = TooltipFolder,
        Visible = false,
    })

    Library:Create('UICorner', {
        CornerRadius = UDim.new(0, 2),
        Parent = Tooltip,
    })

    Library:AddStroke(Tooltip, Border, Brown)

    local Label = Library:Create("TextLabel", {
        Parent = Tooltip;
        BackgroundColor3 = Color3.fromRGB(255, 255, 255);
        BackgroundTransparency = 1;
        BorderColor3 = Color3.fromRGB(0, 0, 0);
        Position = UDim2.new(0, 5, 0, 1);
        Size = UDim2.new(1, 0, 1, 0);
        BorderSizePixel = 0;
        Text = InfoString;
        TextColor3 = Library.FontColor;
        TextXAlignment = Enum.TextXAlignment.Left;
        ZIndex = Tooltip.ZIndex + 1;
    })

    local IsHovering = false;

    TargetParent.MouseEnter:Connect(function()
        IsHovering = true
        Tooltip.Position = UDim2.new(0, Mouse.X + 15, 0, Mouse.Y + 12)
        Tooltip.Visible = true

        while IsHovering do
            RunService.Heartbeat:Wait()

            Tooltip.Position = UDim2.new(0, Mouse.X + 15, 0, Mouse.Y + 12)
        end

    end);

    TargetParent.MouseLeave:Connect(function()
        IsHovering = false;
        Tooltip.Visible = false;
    end);

end;

function Utility:Tween(instance, properties, duration, ...)
    TweenService:Create(instance, TweenInfo(duration, ...), properties):Play();
end;


function Library:AddGameStat()

    if not game.CoreGui.NotificationsBT.Notification_Area:FindFirstChild('GameStat') then

        local GameStat = Instance.new("Frame")

        GameStat.Name = "GameStat"
        GameStat.Parent = Notification_Area
        GameStat.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        GameStat.BackgroundTransparency = 0.700
        GameStat.BorderColor3 = Color3.fromRGB(0, 0, 0)
        GameStat.BorderSizePixel = 0
        GameStat.Size = UDim2.new(0, 200, 0, 85)
        
        Library:Create('UICorner', {
            CornerRadius = UDim.new(0, 5),
            Parent = GameStat,
        })

        Library:Create('TextLabel', {
            Parent = GameStat,
            BackgroundColor3 = Color3.fromRGB(255, 255, 255),
            BackgroundTransparency = 1.000,
            BorderColor3 = Color3.fromRGB(0, 0, 0),
            BorderSizePixel = 0,
            Size = UDim2.new(0, 200, 0, 30),
            Font = Enum.Font.SourceSansBold,
            Text = "Game Stat",
            TextColor3 = Color3.fromRGB(255, 255, 255),
            TextSize = 16.000,
        })
        
        local GameStatText = Library:Create('TextBox', {
            Parent = GameStat,
            BackgroundColor3 = Color3.fromRGB(255, 255, 255),
            BackgroundTransparency = 1.000,
            BorderColor3 = Color3.fromRGB(0, 0, 0),
            BorderSizePixel = 0,
            Position = UDim2.new(0, 5, 0, 30),
            Size = UDim2.new(1, -10, 0, 45),
            ClearTextOnFocus = false,
            Font = Enum.Font.SourceSansBold,
            MultiLine = true,
            Text = "",
            TextColor3 = Color3.fromRGB(255, 255, 255),
            TextSize = 12.000,
            TextXAlignment = Enum.TextXAlignment.Left,
            TextYAlignment = Enum.TextYAlignment.Top,
            TextEditable = false,
        })

        local GameSTatFunc = {};

        function GameSTatFunc:Refresh()
            local ping = game:GetService("Stats").PerformanceStats.Ping:GetValueString()
            local gpu = game:GetService("Stats").PerformanceStats.GPU:GetValueString()
            local cpu = game:GetService("Stats").PerformanceStats.CPU:GetValueString()
            local memory = game:GetService("Stats").PerformanceStats.Memory:GetValueString()
            GameStatText.Text = "Ping   : "..ping.."\nGpu    : "..gpu.."\nCpu    : "..cpu.."\nMemory : "..memory
        end

        game:GetService('RunService').Heartbeat:Connect(function()
            GameSTatFunc:Refresh()
        end)

        return GameSTatFunc

    else
        game.CoreGui.NotificationsBT.Notification_Area:FindFirstChild('GameStat'):Destroy()
    end
    
end

function Library:SetNotify(title, text, times)

    local XSize, YSize = Library:GetTextBounds(text, Library.Font, 14);

    YSize = YSize + 7
    
    local NotificationTemplate = Library:Create('Frame', {
        Parent = Notification_Area,
        BackgroundColor3 = Color3.fromRGB(255, 255, 255),
        BackgroundTransparency = 1.000,
        BorderColor3 = Color3.fromRGB(0, 0, 0),
        BorderSizePixel = 0,
        Size = UDim2.new(0, 0, 0, YSize),
        ClipsDescendants = true,
        ZIndex = 100,
    })

    local Notification_Inner = Library:Create('Frame', {
        Parent = NotificationTemplate,
        BackgroundColor3 = Library.MainColor,
        BorderColor3 = Library.OutlineColor,
        BorderMode = Enum.BorderMode.Inset,
        ClipsDescendants = true,
        Size = UDim2.new(1, 0, 1, 0),
        ZIndex = 101,
    })

    local Notification_Label = Library:Create('TextLabel', {
        Parent = Notification_Inner,
        BackgroundColor3 = Color3.fromRGB(255, 255, 255),
        BackgroundTransparency = 1.000,
        BorderColor3 = Color3.fromRGB(0, 0, 0),
        BorderSizePixel = 0,
        Position = UDim2.new(0, 5, 0, 0),
        Size = UDim2.new(1, -10, 0, 30),
        Font = Enum.Font.SourceSansBold,
        Text = title,
        TextColor3 = Color3.fromRGB(255, 255, 255),
        TextSize = 14.000,
        ZIndex = 102,
    })

    local Notification_Text = Library:Create('TextLabel', {
        Parent = Notification_Inner,
        BackgroundColor3 = Color3.fromRGB(255, 255, 255),
        BackgroundTransparency = 1.000,
        BorderColor3 = Color3.fromRGB(0, 0, 0),
        BorderSizePixel = 0,
        Position = UDim2.new(0, 5, 0, 35),
        Size = UDim2.new(1, -10, 1, -40),
        Font = Enum.Font.SourceSansBold,
        Text = text,
        TextColor3 = Color3.fromRGB(255, 255, 255),
        TextSize = 12.000,
        ZIndex = 103,
    })

    pcall(NotificationTemplate.TweenSize, NotificationTemplate, UDim2.new(0, XSize + 8 + 4, 0, YSize + 80), 'Out', 'Quad', 0.4, true);

    task.spawn(function()
        wait(times or 3);

        pcall(NotificationTemplate.TweenSize, NotificationTemplate, UDim2.new(0, 0, 0, YSize), 'Out', 'Quad', 0.4, true);

        wait(0.4);

        NotificationTemplate:Destroy();
    end);
end

-- Scripts:
function Library:DragEnabled(isTarget) -- BODY.BodyObject 
    local script = Instance.new('LocalScript', isTarget);
    
    local gui = isTarget;
    local dragging = false;
    local dragInput, dragStart, startPos;
    
    local function update(input)
        local delta = input.Position - dragStart;
        local Pos = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y);

        local Tween = TweenService:Create(gui, TweenInfo(.15), {Position = Pos});
        Tween:Play();
    end;
    
    gui.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            dragging = true;
            dragStart = input.Position;
            startPos = gui.Position;
    
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    dragging = false;
                end;
            end);
        end;
    end);
    
    gui.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
            dragInput = input;
        end;
    end);
    
    UserInputService.InputChanged:Connect(function(input)
        if input == dragInput and dragging then
            update(input);
        end;
    end);
end;

local Toggle = Instance.new("Frame")
local UICorner = Instance.new("UICorner")
local ToggleButton = Instance.new("ImageButton")

Toggle.Name = "Toggle"
Toggle.Parent = BLACKTRAP
