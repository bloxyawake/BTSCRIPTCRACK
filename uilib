    --// Addon Tab
    local AddonTab = {}
    local isFirstTab = true

    function AddonTab:AddTab(isTitle, isIcon)

        local Outer_Tab = Library:Create('Frame', {
            Parent = Scrolling_Tab,
            BackgroundColor3 = Color3.fromRGB(240, 240, 240),
            BackgroundTransparency = 1.000,
            BorderColor3 = Color3.fromRGB(0, 0, 0),
            BorderSizePixel = 0,
            Size = UDim2.new(0, 100, 0, 23),
        })

        local Inner_Tab = Library:Create('Frame', {
            Parent = Outer_Tab,
            BackgroundColor3 = Themes.Default.Inner_Tab_Background,
            BorderColor3 = Color3.fromRGB(0, 0, 0),
            BorderSizePixel = 0,
            Position = UDim2.new(0, 3, 0, 3),
            Size = UDim2.new(1, -6, 0, 20),
        })

        Library:AddStroke(Inner_Tab, Border, Themes.Default.Lines)

        local Tab_Icons = Library:Create('ImageLabel', {
            Parent = Inner_Tab,
            BackgroundColor3 = Color3.fromRGB(255, 255, 255),
            BackgroundTransparency = 1.000,
            BorderColor3 = Color3.fromRGB(0, 0, 0),
            BorderSizePixel = 0,
            Position = UDim2.new(0, 2, 0, 3),
            Size = UDim2.new(0, 14, 0, 14),
            Image = isIcon,
            ImageColor3 = Themes.Default.Font,
        })

        local Tab_Buttons = Library:Create('TextButton', {
            Parent = Inner_Tab,
            BackgroundColor3 = Color3.fromRGB(255, 255, 255),
            BackgroundTransparency = 1.000,
            BorderColor3 = Color3.fromRGB(0, 0, 0),
            BorderSizePixel = 0,
            Position = UDim2.new(0, 20, 0, 0),
            Size = UDim2.new(0, 80, 0, 20),
            Font = Enum.Font.SourceSans,
            Text = isTitle,
            TextColor3 = Themes.Default.Font,
            TextSize = 12.000,
            TextXAlignment = Enum.TextXAlignment.Left,
            AutoButtonColor = false,
        })

        local Frame_Section_Inner = Library:Create('Frame', {
            Parent = Frame_Section_Outer,
            BackgroundColor3 = Color3.fromRGB(255, 255, 255),
            BackgroundTransparency = 1.000,
            BorderColor3 = Color3.fromRGB(0, 0, 0),
            BorderSizePixel = 0,
            Size = UDim2.new(1, 0, 1, 0),
            Position = UDim2.new(0, 0, 0, 0),
            Visible = false,
        })

        local Frame_Section_Inner_Layout = Library:Create('UIListLayout', {
            Parent = Frame_Section_Inner,
            SortOrder = Enum.SortOrder.LayoutOrder,
            FillDirection = Enum.FillDirection.Horizontal,
            Padding = UDim.new(0, 5),
        })

        Frame_Section_Inner_Layout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
            Frame_Section_Inner.Size = UDim2.new(0, 0 + Frame_Section_Inner_Layout.AbsoluteContentSize.X + Frame_Section_Inner_Layout.Padding.Offset, 1, 0)
        end)
    
        Frame_Section_Inner.ChildAdded:Connect(function()
            Frame_Section_Inner.Size = UDim2.new(0, 0 + Frame_Section_Inner_Layout.AbsoluteContentSize.X + Frame_Section_Inner_Layout.Padding.Offset, 1, 0)
        end)

        if isFirstTab then
            isFirstTab = false;
            Inner_Tab.BackgroundColor3 = Themes.Default.Background_Active
            Tab_Icons.ImageColor3 = Themes.Default.Item_Active
            Tab_Buttons.TextColor3 = Themes.Default.Item_Active
            MainTabLabel.Text = "Tabs : "..isTitle
            Frame_Section_Inner.Visible = true
        end

        Tab_Buttons.MouseButton1Click:Connect(function()
            for _,v in next, Scrolling_Tab:GetChildren() do
                if v:IsA('Frame') then
                    for ___,k in next, v:GetChildren() do
                        if k:IsA('Frame') then

                            Utility:Tween(k, {BackgroundColor3 = Themes.Default.Background_Deactive}, .10)
                            MainTabLabel.Text = ""

                            for zz,xx in next, k:GetChildren() do
                                if xx:IsA('TextButton') then
                                    Utility:Tween(xx, {TextColor3 = Themes.Default.Item_Deactive}, .10);
                                end
                                if xx:IsA('ImageLabel') then
                                    Utility:Tween(xx, {ImageColor3 = Themes.Default.Item_Deactive}, .10);
                                end
                            end
                        end
                    end
                end
            end

            for _,v in next, Frame_Section_Outer:GetChildren() do
                if v:IsA('Frame') then
                    Utility:Tween(v, {Position = UDim2.new(0, 500, 0, 0)}, .10);
                    Frame_Section_Inner.Visible = false
                end
            end

            Utility:Tween(Inner_Tab, {BackgroundColor3 = Themes.Default.Background_Active}, .10);
            Utility:Tween(Tab_Icons, {ImageColor3 = Themes.Default.Item_Active}, .10);
            Utility:Tween(Tab_Buttons, {TextColor3 = Themes.Default.Item_Active}, .10);
            MainTabLabel.Text = "Tabs : "..isTitle
            Frame_Section_Inner.Visible = true
            Utility:Tween(Frame_Section_Inner, {Position = UDim2.new(0, 0, 0, 0)}, .10);
        end)


        --// Addon Section
        local AddonSection = {}
        function AddonSection:AddSection()

            local Section_Scroll = Library:Create('ScrollingFrame', {
                Parent = Frame_Section_Inner,
                Active = true,
                BackgroundColor3 = Themes.Default.Scrolling_Tab_Background,
                BorderColor3 = Color3.fromRGB(0, 0, 0),
                BorderSizePixel = 0,
                Size = UDim2.new(0, 230, 0, 265),
                ScrollBarThickness = 0,
            })

            Library:AddStroke(Section_Scroll, Border, Themes.Default.Lines)

            local Section_Scroll_Layout = Library:Create('UIListLayout', {
                Parent = Section_Scroll,
                SortOrder = Enum.SortOrder.LayoutOrder,
                Padding = UDim.new(0, 10),
            })

            Section_Scroll_Layout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
                Section_Scroll.CanvasSize = UDim2.new(0, 0, 0, Section_Scroll_Layout.Padding.Offset + Section_Scroll_Layout.AbsoluteContentSize.Y + 6);
            end)
        
            Section_Scroll.ChildAdded:Connect(function()
                Section_Scroll.CanvasSize = UDim2.new(0, 0, 0, Section_Scroll_Layout.Padding.Offset + Section_Scroll_Layout.AbsoluteContentSize.Y + 6);
            end)
            
            --// Addon Group
            local AddonGroup = {}
            function AddonGroup:AddGroup(isGroupTitle)

                local Group_Outer = Library:Create('Frame', {
                    Parent = Section_Scroll,
                    BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                    BackgroundTransparency = 1.000,
                    BorderColor3 = Color3.fromRGB(0, 0, 0),
                    BorderSizePixel = 0,
                    Size = UDim2.new(1, 0, 0, 36),
                })

                local Group_Inner = Library:Create('Frame', {
                    Parent = Group_Outer,
                    BackgroundColor3 = Themes.Default.Group_Inner_Background,
                    BorderColor3 = Color3.fromRGB(0, 0, 0),
                    BorderSizePixel = 0,
                    Position = UDim2.new(0, 5, 0, 6),
                    Size = UDim2.new(1, -10, 0, 31),
                })

                Library:AddStroke(Group_Inner, Border, Themes.Default.Lines)

                local Group_Inner_Layout = Library:Create('UIListLayout', {
                    Parent = Group_Inner,
                    SortOrder = Enum.SortOrder.LayoutOrder,
                    Padding = UDim.new(0,5),
                })

                Library:Create('TextLabel', {
                    Parent = Group_Inner,
                    BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                    BackgroundTransparency = 1.000,
                    BorderColor3 = Color3.fromRGB(0, 0, 0),
                    BorderSizePixel = 0,
                    Size = UDim2.new(1, 0, 0, 20),
                    Font = Enum.Font.Arcade,
                    Text = isGroupTitle,
                    TextColor3 = Themes.Default.Font,
                    TextSize = 10.000,
                })

                Group_Outer.Size = UDim2.new(1, 0, 0, Group_Inner_Layout.AbsoluteContentSize.Y + Group_Inner_Layout.Padding.Offset + 6)
                Group_Inner.Size = UDim2.new(1, -10, 0, Group_Inner_Layout.AbsoluteContentSize.Y + Group_Inner_Layout.Padding.Offset + 6)
                Group_Inner_Layout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
                    Group_Outer.Size = UDim2.new(1, 0, 0, Group_Inner_Layout.AbsoluteContentSize.Y + Group_Inner_Layout.Padding.Offset + 6)
                    Group_Inner.Size = UDim2.new(1, -10, 0, Group_Inner_Layout.AbsoluteContentSize.Y + Group_Inner_Layout.Padding.Offset + 6)
                end);
                
                local function SectionScrollChanged()
                    local GIL = 0
                    GIL = Group_Inner_Layout.AbsoluteContentSize.Y
                    
                    Section_Scroll.CanvasSize = UDim2.new(0, 0, 0, GIL + Group_Inner_Layout.Padding.Offset + 6)
                end

                local function groupsizechange()

                    Group_Outer.Size = UDim2.new(1, 0, 0, Group_Inner_Layout.AbsoluteContentSize.Y + Group_Inner_Layout.Padding.Offset + 6)
                    Group_Inner.Size = UDim2.new(1, -10, 0, Group_Inner_Layout.AbsoluteContentSize.Y + Group_Inner_Layout.Padding.Offset + 6)
                    
                end

                SectionScrollChanged()
                groupsizechange()

                --// Addon Menu
                local AddonMenu = {}
                function AddonMenu:AddButton(Info)
                    local Button = {
                        Text = Info.Text;
                        Tooltip = Info.Tooltip;
                        Callback = Info.Callback or function() end;
                    }

                    local Button_Outer = Library:Create('Frame', {
                        Parent = Group_Inner,
                        BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                        BackgroundTransparency = 1.000,
                        BorderColor3 = Color3.fromRGB(0, 0, 0),
                        BorderSizePixel = 0,
                        Size = UDim2.new(1, 0, 0, 20),
                    })

                    local Button_Inner = Library:Create('Frame', {
                        Parent = Button_Outer,
                        AnchorPoint = Vector2.new(0.5, 0.5),
                        BackgroundColor3 = Themes.Default.Background_Deactive,
                        BorderColor3 = Color3.fromRGB(0, 0, 0),
                        BorderSizePixel = 0,
                        Position = UDim2.new(0, 110, 0, 10),
                        Size = UDim2.new(1, -10, 1, 0),
                    })

                    Library:AddStroke(Button_Inner, Border, Themes.Default.Lines)

                    local Main_Button = Library:Create('TextButton', {
                        Parent = Button_Inner,
                        BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                        BackgroundTransparency = 1.000,
                        BorderColor3 = Color3.fromRGB(0, 0, 0),
                        BorderSizePixel = 0,
                        Size = UDim2.new(1, 0, 1, 0),
                        Font = Enum.Font.SourceSans,
                        TextColor3 = Themes.Default.Item_Deactive,
                        TextSize = 12.000,
                        AutoButtonColor = false,
                        Text = Info.Text,
                    })

                    Main_Button.MouseEnter:Connect(function()
                        Utility:Tween(Button_Inner, {BackgroundColor3 = Themes.Default.Background_Active}, .15);
                        Utility:Tween(Main_Button, {TextColor3 = Themes.Default.Item_Active}, .15);
                        Utility:Tween(Button_Inner, {Size = UDim2.new(1, -20, 1, 0)}, .15);
                    end);

                    Main_Button.MouseLeave:Connect(function()
                        Utility:Tween(Button_Inner, {BackgroundColor3 = Themes.Default.Background_Deactive}, .15);
                        Utility:Tween(Main_Button, {TextColor3 = Themes.Default.Item_Deactive}, .15);
                        Utility:Tween(Button_Inner, {Size = UDim2.new(1, -10, 1, 0)}, .15);
                    end);

                    Main_Button.MouseButton1Down:Connect(function()
                        Utility:Tween(Button_Inner, {BackgroundColor3 = Themes.Default.Background_Deactive}, .15);
                        Utility:Tween(Main_Button, {TextColor3 = Themes.Default.Item_Deactive}, .15);
                        Utility:Tween(Button_Inner, {Size = UDim2.new(1, -10, 1, 0)}, .15);
                    end)

                    Main_Button.MouseButton1Up:Connect(function()
                        Utility:Tween(Button_Inner, {BackgroundColor3 = Themes.Default.Background_Active}, .15);
                        Utility:Tween(Main_Button, {TextColor3 = Themes.Default.Item_Active}, .15);
                        Utility:Tween(Button_Inner, {Size = UDim2.new(1, -20, 1, 0)}, .15);
                    end)

                    Main_Button.MouseButton1Click:Connect(function()
                        Button.Callback();
                    end);

                    if type(Info.Tooltip) == 'string' then
                        Library:AddToolTip(Info.Tooltip, Button_Inner);
                    end;

                    return Button;
                end

                --// Create Checkbox
                function AddonMenu:AddCheckbox(Idx, Info)
                    local Checkbox = {
                        Type = 'Checkbox';
                        Text = Info.Text;
                        Tooltip = Info.Tooltip;
                        Value = Info.Default or false;
                        Callback = Info.Callback or function(Value) end;
                    };

                    local Frame_Outer = Library:Create('Frame', {
                        Parent = Group_Inner,
                        BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                        BackgroundTransparency = 1.000,
                        BorderColor3 = Color3.fromRGB(0, 0, 0),
                        BorderSizePixel = 0,
                        Size = UDim2.new(1, 0, 0, 20),
                    })

                    local Frame_Inner = Library:Create('Frame', {
                        Parent = Frame_Outer,
                        BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                        BackgroundTransparency = 1.000,
                        BorderColor3 = Color3.fromRGB(0, 0, 0),
                        BorderSizePixel = 0,
                        Position = UDim2.new(0, -1, 0, 0),
                        Size = UDim2.new(1, -4, 1, 0),
                    })

                    local Checkbox_Button = Library:Create('TextButton', {
                        Parent = Frame_Inner,
                        BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                        BackgroundTransparency = 1.000,
                        BorderColor3 = Color3.fromRGB(0, 0, 0),
                        BorderSizePixel = 0,
                        Size = UDim2.new(1, 0, 0, 20),
                        AutoButtonColor = false,
                        Font = Enum.Font.SourceSans,
                        Text = "",
                        TextColor3 = Color3.fromRGB(0, 0, 0),
                        TextSize = 14.000,
                    })

                    local Check_Outer = Library:Create('Frame', {
                        Parent = Checkbox_Button,
                        BackgroundColor3 = Themes.Default.Background_Deactive,
                        BorderColor3 = Color3.fromRGB(0, 0, 0),
                        BorderSizePixel = 0,
                        Position = UDim2.new(0, 6, 0, 6),
                        Size = UDim2.new(0, 12, 0, 12),
                    })

                    Library:AddStroke(Check_Outer, Border, Themes.Default.Lines)

                    local Check_Inner = Library:Create('ImageLabel', {
                        Parent = Check_Outer,
                        BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                        BackgroundTransparency = 1.000,
                        BorderColor3 = Color3.fromRGB(0, 0, 0),
                        BorderSizePixel = 0,
                        Size = UDim2.new(0, 12, 0, 12),
                        Image = "http://www.roblox.com/asset/?id=18664334374",
                        ImageColor3 = Themes.Default.Item_Active,
                        Visible = false,
                    })

                    local Checkbox_Label = Library:Create('TextLabel', {
                        Parent = Frame_Inner,
                        BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                        BackgroundTransparency = 1.000,
                        BorderColor3 = Color3.fromRGB(0, 0, 0),
                        BorderSizePixel = 0,
                        Position = UDim2.new(0, 26, 0, 6),
                        Size = UDim2.new(0, 190, 0, 12),
                        Font = Enum.Font.SourceSans,
                        Text = Info.Text,
                        TextColor3 = Themes.Default.Item_Deactive,
                        TextSize = 12.000,
                        TextXAlignment = Enum.TextXAlignment.Left,
                    })

                    
                    --[[local Checkbox_function = false;
                        if Info.Default then
                        Check_Outer.BackgroundColor3 = Color3.fromRGB(220, 220, 255)
                        Check_Inner.Visible = true
                        Checkbox_function = not Checkbox_function;
                        Info.Callback(Checkbox_function);
                    end;]]

                    Checkbox_Button.MouseEnter:Connect(function()
                        Utility:Tween(Checkbox_Label, {TextColor3 = Themes.Default.Item_Active}, .15)
                    end)

                    Checkbox_Button.MouseLeave:Connect(function()
                        Utility:Tween(Checkbox_Label, {TextColor3 = Themes.Default.Item_Deactive}, .15)
                    end)

                    function Checkbox:SetValue(Bool)
                        Bool = (not not Bool);
                        Checkbox.Value = Bool;

                        if not Bool then
                            Check_Outer.BackgroundColor3 = Themes.Default.Background_Deactive
                            Check_Inner.Visible = false
                        else
                            Check_Outer.BackgroundColor3 = Themes.Default.Background_Active
                            Check_Inner.Visible = true
                        end

                        Checkbox.Callback(Checkbox.Value);
                    end

                    Checkbox_Button.MouseButton1Click:Connect(function()
                        if not Checkbox.Value then
                            Check_Outer.BackgroundColor3 = Themes.Default.Background_Active
                            Check_Inner.Visible = true
                        else
                            Check_Outer.BackgroundColor3 = Themes.Default.Background_Deactive
                            Check_Inner.Visible = false
                        end;

                        Library:AttemptSave();
                        Checkbox.Value = not Checkbox.Value;
                        Checkbox.Callback(Checkbox.Value);
                    end);

                    if type(Info.Tooltip) == 'string' then
                        Library:AddToolTip(Info.Tooltip, Checkbox_Label);
                    end;
                    
                    Checkboxs[Idx] = Checkbox;

                    return Checkbox;
                end

                
                --// Add Dropdown
                function AddonMenu:AddDropdown(Idx, Info)

                    local Dropdown = {
                        Type = 'Dropdown';
                        Text = Info.Text;
                        Tooltip = Info.Tooltip;
                        List = Info.List;
                        Multi = Info.Multi;
                        Value = Info.Multi and {};
                        Callback = Info.Callback or function(Value) end;
                    };

                    local Dropdown_Outer = Library:Create('Frame', {
                        Parent = Group_Inner,
                        BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                        BackgroundTransparency = 1.000,
                        BorderColor3 = Color3.fromRGB(0, 0, 0),
                        BorderSizePixel = 0,
                        Size = UDim2.new(1, 0, 0, 20),
                    })

                    local Dropdown_Inner = Library:Create('Frame', {
                        Parent = Dropdown_Outer,
                        BackgroundColor3 = Themes.Default.Background_Deactive,
                        BorderColor3 = Color3.fromRGB(0, 0, 0),
                        BorderSizePixel = 0,
                        Position = UDim2.new(0, 5, 0, 0),
                        Size = UDim2.new(1, -10, 0, 20),
                    })

                    Library:AddStroke(Dropdown_Inner, Border, Themes.Default.Lines)

                    local Title_Dropdown = Library:Create('TextLabel', {
                        Parent = Dropdown_Inner,
                        BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                        BackgroundTransparency = 1.000,
                        BorderColor3 = Color3.fromRGB(0, 0, 0),
                        BorderSizePixel = 0,
                        Position = UDim2.new(0, 3, 0, 0),
                        Size = UDim2.new(0, 190, 0, 20),
                        Font = Enum.Font.SourceSans,
                        Text = Info.Text,
                        TextColor3 = Themes.Default.Item_Deactive,
                        TextSize = 12.000,
                        TextXAlignment = Enum.TextXAlignment.Left,
                    })

                    local Arrow_Button = Library:Create('TextButton', {
                        Parent = Dropdown_Inner,
                        BackgroundColor3 = Themes.Default.Background_Active,
                        BorderColor3 = Color3.fromRGB(0, 0, 0),
                        BorderSizePixel = 0,
                        Position = UDim2.new(0, 198, 0, 0),
                        Size = UDim2.new(0, 12, 0, 20),
                        AutoButtonColor = false,
                        Font = Enum.Font.SourceSans,
                        Text = "",
                        TextColor3 = Color3.fromRGB(0, 0, 0),
                        TextSize = 14.000,
                    })

                    Library:AddStroke(Arrow_Button, Border, Themes.Default.Lines)

                    local Arrow_Icon = Library:Create('ImageLabel', {
                        Parent = Arrow_Button,
                        BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                        BackgroundTransparency = 1.000,
                        BorderColor3 = Color3.fromRGB(0, 0, 0),
                        BorderSizePixel = 0,
                        Position = UDim2.new(0, 0, 0, 4),
                        Size = UDim2.new(0, 12, 0, 12),
                        Image = "http://www.roblox.com/asset/?id=293296862",
                        ImageColor3 = Themes.Default.Item_Active,
                        Rotation = 0,
                    })

                    local Main_Dropdown = Library:Create('Frame', {
                        Parent = Group_Inner,
                        BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                        BackgroundTransparency = 1.000,
                        BorderColor3 = Color3.fromRGB(0, 0, 0),
                        BorderSizePixel = 0,
                        Size = UDim2.new(1, 0, 0, 0),
                        Visible = false
                    })

                    local Frame_Dropdown = Library:Create('Frame', {
                        Parent = Main_Dropdown,
                        BackgroundColor3 = Themes.Default.Background_Deactive,
                        BorderColor3 = Color3.fromRGB(0, 0, 0),
                        BorderSizePixel = 0,
                        Position = UDim2.new(0, 5, 0, -5),
                        Size = UDim2.new(1, -23, 0, 0),
                    })

                    Library:AddStroke(Frame_Dropdown, Border, Themes.Default.Lines)

                    local Frame_Dropdown_Layout = Library:Create('UIListLayout', {
                        Parent = Frame_Dropdown,
                        SortOrder = Enum.SortOrder.LayoutOrder,
                        Padding = UDim.new(0,3)
                    })

                    local Dropdown_Button = true;
                    Arrow_Button.MouseButton1Click:Connect(function()
                        if Dropdown_Button then
                            Arrow_Icon.Rotation = 180;
                            Main_Dropdown.Visible = true;

                            Utility:Tween(Main_Dropdown, {Size = UDim2.new(1, 0, 0, 0 + Frame_Dropdown_Layout.AbsoluteContentSize.Y)}, .15);
                            Utility:Tween(Frame_Dropdown, {Size = UDim2.new(1, -23, 0, 0 + Frame_Dropdown_Layout.AbsoluteContentSize.Y)}, .15);

                        else
                            Arrow_Icon.Rotation = 0;
                            Utility:Tween(Main_Dropdown, {Size = UDim2.new(1, 0, 0, 0)}, .15);
                            Utility:Tween(Frame_Dropdown, {Size = UDim2.new(1, -23, 0, 0)}, .15);
                            Main_Dropdown.Visible = false;
                        end;
                        Dropdown_Button = not Dropdown_Button;
                        
                    end);

                    if type(Info.Tooltip) == 'string' then
                        Library:AddToolTip(Info.Tooltip, Title_Dropdown);
                    end;

                    function Dropdown:Display()
                        local Values = Dropdown.List;
                        local Str = '';
            
                        if Info.Multi then
                            for Idx, Value in next, Values do
                                if Dropdown.Value[Value] then
                                    Str = Str .. Value .. ', ';
                                end;
                            end;
            
                            Str = Str:sub(1, #Str - 2);
                        else
                            Str = Dropdown.Value or '';
                        end;
            
                        Title_Dropdown.Text = Dropdown.Text..' - '..Str
                    end;

                    function Dropdown:GetActiveValues()
                        if Info.Multi then
                            local T = {};
            
                            for Value, Bool in next, Dropdown.Value do
                                table.insert(T, Value);
                            end;
            
                            return T;
                        else
                            return Dropdown.Value and 1 or 0;
                        end;
                    end;

                    function Dropdown:BuildDropdownList()

                        local Values = Dropdown.List
                        local Buttons = {};

                        for ___,oldlist in pairs(Frame_Dropdown:GetChildren()) do
                            if oldlist:IsA('Frame') then
                                oldlist:Destroy()
                                Title_Dropdown.Text = Dropdown.Text
                            end
                        end

                        for Idx, Value in next, Values do

                            local Table = {};

                            local List_Frame = Library:Create('Frame', {
                                Parent = Frame_Dropdown,
                                BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                                BackgroundTransparency = 1.000,
                                BorderColor3 = Color3.fromRGB(0, 0, 0),
                                BorderSizePixel = 0,
                                Size = UDim2.new(1, 0, 0, 20),
                            })

                            local Dropdown_Check_Outer = Library:Create('Frame', {
                                Parent = List_Frame,
                                BackgroundColor3 = Themes.Default.Background_Deactive,
                                BorderColor3 = Color3.fromRGB(0, 0, 0),
                                BorderSizePixel = 0,
                                Position = UDim2.new(0, 6, 0, 4),
                                Size = UDim2.new(0, 12, 0, 12),
                            })
                        
                            Library:AddStroke(Dropdown_Check_Outer, Border, Themes.Default.Lines)

                            local Dropdown_Check_Inner = Library:Create('ImageLabel', {
                                Parent = Dropdown_Check_Outer,
                                BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                                BackgroundTransparency = 1.000,
                                BorderColor3 = Color3.fromRGB(0, 0, 0),
                                BorderSizePixel = 0,
                                Size = UDim2.new(0, 12, 0, 12),
                                Image = "http://www.roblox.com/asset/?id=18664334374",
                                ImageColor3 = Themes.Default.Item_Active,
                                Visible = false,
                            })
                        
                            local Dropdown_List_Button = Library:Create('TextButton', {
                                Parent = List_Frame,
                                BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                                BackgroundTransparency = 1.000,
                                BorderColor3 = Color3.fromRGB(0, 0, 0),
                                BorderSizePixel = 0,
                                Size = UDim2.new(1, 0, 1, 0),
                                Font = Enum.Font.SourceSans,
                                Text = "",
                                TextColor3 = Color3.fromRGB(0, 0, 0),
                                TextSize = 14.000,
                            })
                        
                            local Dropdown_List_Title = Library:Create('TextLabel', {
                                Parent = List_Frame,
                                BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                                BackgroundTransparency = 1.000,
                                BorderColor3 = Color3.fromRGB(0, 0, 0),
                                BorderSizePixel = 0,
                                Position = UDim2.new(0, 23, 0, 0),
                                Size = UDim2.new(1, -23, 1, 0),
                                Font = Enum.Font.SourceSans,
                                TextColor3 = Themes.Default.Item_Deactive,
                                TextSize = 12.000,
                                TextXAlignment = Enum.TextXAlignment.Left,
                                Text = Value,
                            })

                            Dropdown_List_Button.MouseEnter:Connect(function()
                                Utility:Tween(Dropdown_List_Title, {TextColor3 = Themes.Default.Item_Active}, 0.15);
                            end);
                        
                            Dropdown_List_Button.MouseLeave:Connect(function()
                                Utility:Tween(Dropdown_List_Title, {TextColor3 = Themes.Default.Item_Deactive}, 0.15);
                            end);

                            local Selected;

                            if Info.Multi then
                                Selected = Dropdown.Value[Value];
                            else
                                Selected = Dropdown.Value == Value;
                            end;

                            function Table:UpdateButton()
                                if Info.Multi then
                                    Selected = Dropdown.Value[Value];

                                    if Selected then
                                        Dropdown_Check_Outer.BackgroundColor3 = Themes.Default.Background_Active
                                        Dropdown_Check_Inner.Visible = true
                                    else
                                        Dropdown_Check_Outer.BackgroundColor3 = Themes.Default.Background_Deactive
                                        Dropdown_Check_Inner.Visible = false
                                    end
                                else
                                    Selected = Dropdown.Value == Value;
                                    if Selected then
                                        Dropdown_Check_Outer.BackgroundColor3 = Themes.Default.Background_Active
                                        Dropdown_Check_Inner.Visible = true
                                    else
                                        Dropdown_Check_Outer.BackgroundColor3 = Themes.Default.Background_Deactive
                                        Dropdown_Check_Inner.Visible = false
                                    end
                                end;
                            end;

                            Dropdown_List_Button.MouseButton1Click:Connect(function()

                                local Try = not Selected;

                                if Dropdown:GetActiveValues() == 1 and (not Try) then
                                else
                                    if Info.Multi then
                                        Selected = Try;
        
                                        if Selected then
                                            Dropdown.Value[Value] = true;
                                        else
                                            Dropdown.Value[Value] = nil;
                                        end;
                                    else
                                        Selected = Try;
        
                                        if Selected then
                                            Dropdown.Value = Value;
                                        else
                                            Dropdown.Value = nil;
                                        end;
        
                                        for _, OtherButton in next, Buttons do
                                            OtherButton:UpdateButton();
                                        end;
                                    end;
        
                                    Table:UpdateButton();
                                    Dropdown:Display();
        
                                    Dropdown.Callback(Dropdown.Value);
        
                                    Library:AttemptSave();
                                end;
                            end);

                            Table:UpdateButton();
                            Dropdown:Display();

                            Buttons[List_Frame] = Table;

                        end
                    end

                    function Dropdown:UpdateValues(NewValues)
                        if NewValues then
                            Dropdown.List = NewValues;
                        end;
            
                        Dropdown:BuildDropdownList();
                    end;

                    function Dropdown:SetValue(Val)
                        if Dropdown.Multi then
                            local nTable = {};
            
                            for Value, Bool in next, Val do
                                if table.find(Dropdown.List, Value) then
                                    nTable[Value] = true
                                end;
                            end;
            
                            Dropdown.Value = nTable;
                        else
                            if (not Val) then
                                Dropdown.Value = nil;
                            elseif table.find(Dropdown.List, Val) then
                                Dropdown.Value = Val;
                            end;
                        end;
            
                        Dropdown:BuildDropdownList();
            
                        Dropdown.Callback(Dropdown.Value);
                    end;


                    --[[function Dropdown:SetValue(Val)

                        if table.find(Dropdown.List, Val) then
                            Dropdown.Value = Val
                            Title_Dropdown.Text = Dropdown.Text.." - "..Val
                        end;

                        Dropdown.Callback(Dropdown.Value);

                    end]]

                    Dropdown:BuildDropdownList();
                    Dropdown:Display();

                    local Defaults = {}

                    if type(Info.Default) == 'string' then
                        local Idx = table.find(Dropdown.List, Info.Default)
                        if Idx then
                            table.insert(Defaults, Idx)
                        end
                    elseif type(Info.Default) == 'table' then
                        for _, Value in next, Info.Default do
                            local Idx = table.find(Dropdown.List, Value)
                            if Idx then
                                table.insert(Defaults, Idx)
                            end
                        end
                    elseif type(Info.Default) == 'number' and Dropdown.List[Info.Default] ~= nil then
                        table.insert(Defaults, Info.Default)
                    end

                    if next(Defaults) then
                        for i = 1, #Defaults do
                            local Index = Defaults[i]
                            if Info.Multi then
                                Dropdown.Value[Dropdown.List[Index]] = true
                            else
                                Dropdown.Value = Dropdown.List[Index];
                            end
            
                            if (not Info.Multi) then break end
                        end
            
                        Dropdown:BuildDropdownList();
                        Dropdown:Display();
                    end


                    Options[Idx] = Dropdown
                    return Dropdown
                end

                function AddonMenu:AddInput(Idx, Info)
                    local Input = {
                        Type = 'Input';
                        Text = Info.Text;
                        Tooltip = Info.Tooltip;
                        Numeric = Info.Numeric or false;
                        Finished = Info.Finished or false;
 
                        Value = Info.Default or '';
                        Callback = Info.Callback or function(Value) end;
                    };

                    local Input_Outer = Library:Create('Frame', {
                        Parent = Group_Inner,
                        BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                        BackgroundTransparency = 1.000,
                        BorderColor3 = Color3.fromRGB(0, 0, 0),
                        BorderSizePixel = 0,
                        Size = UDim2.new(1, 0, 0, 20),
                    })

                    local Input_Inner = Library:Create('Frame', {
                        Parent = Input_Outer,
                        BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                        BackgroundTransparency = 1.000,
                        BorderColor3 = Color3.fromRGB(0, 0, 0),
                        BorderSizePixel = 0,
                        Position = UDim2.new(0, 5, 0, 0),
                        Size = UDim2.new(1, -10, 1, 0),
                    })

                    local InputBox = Library:Create('TextBox', {
                        Parent = Input_Inner,
                        BackgroundColor3 = Themes.Default.Background_Active,
                        BorderColor3 = Color3.fromRGB(0, 0, 0),
                        BorderSizePixel = 0,
                        Position = UDim2.new(0, 0, 0, 2),
                        Size = UDim2.new(0, 100, 0, 16),
                        Font = Enum.Font.SourceSans,
                        Text = Info.Default or '',
                        TextColor3 = Themes.Default.Item_Deactive,
                        TextSize = 12.000,
                        ClearTextOnFocus = false,
                        PlaceholderText = 'Input here...',
                        PlaceholderColor3 = Themes.Default.Item_Deactive,
                    })

                    Library:AddStroke(InputBox, Border, Themes.Default.Lines)

                    local Input_Label = Library:Create('TextLabel', {
                        Parent = Input_Inner,
                        BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                        BackgroundTransparency = 1.000,
                        BorderColor3 = Color3.fromRGB(0, 0, 0),
                        BorderSizePixel = 0,
                        Position = UDim2.new(0, 105, 0, 0),
                        Size = UDim2.new(0, 105, 0, 20),
                        Font = Enum.Font.SourceSans,
                        Text = Info.Text,
                        TextColor3 = Themes.Default.Item_Deactive,
                        TextSize = 12.000,
                        TextXAlignment = Enum.TextXAlignment.Left,
                    })

                    function Input:SetValue(addtext)

                        if Input.Numeric then
                            if (not tonumber(addtext)) then
                                addtext = Input.Value
                            end
                        end

                        Input.Value = addtext
                        InputBox.Text = addtext

                        Input.Callback(Input.Value);
                    end

                    if Input.Finished then
                        InputBox.FocusLost:Connect(function(enter)
                            if not enter then return end
            
                            Input:SetValue(InputBox.Text);
                            Library:AttemptSave();
                        end)
                    else
                        InputBox:GetPropertyChangedSignal('Text'):Connect(function()
                            Input:SetValue(InputBox.Text);
                            Library:AttemptSave();
                        end);
                    end

                    --[[InputBox.FocusLost:Connect(function(enter)
                        if not enter then return end

                        Input:SetValue(InputBox.Text)
                        Library:AttemptSave();
                    end);]]

                    if type(Info.Tooltip) == 'string' then
                        Library:AddToolTip(Info.Tooltip, Input_Label)
                    end

                    Options[Idx] = Input
                    return Input
                end

                function AddonMenu:AddLabel(isLabel)
                    local Labels = {}
                    local Label_Outer = Library:Create('Frame', {
                        Parent = Group_Inner,
                        BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                        BackgroundTransparency = 1.000,
                        BorderColor3 = Color3.fromRGB(0, 0, 0),
                        BorderSizePixel = 0,
                        Size = UDim2.new(1, 0, 0, 20),
                    })

                    local Label_Inner = Library:Create('Frame', {
                        Parent = Label_Outer,
                        BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                        BackgroundTransparency = 1.000,
                        BorderColor3 = Color3.fromRGB(0, 0, 0),
                        BorderSizePixel = 0,
                        Position = UDim2.new(0, 5, 0, 0),
                        Size = UDim2.new(1, -10, 1, 0),
                    })

                    local Text_Label = Library:Create('TextLabel', {
                        Parent = Label_Inner,
                        BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                        BackgroundTransparency = 1.000,
                        BorderColor3 = Color3.fromRGB(0, 0, 0),
                        BorderSizePixel = 0,
                        Size = UDim2.new(1, 0, 1, 0),
                        Font = Enum.Font.SourceSans,
                        TextColor3 = Themes.Default.Item_Active,
                        TextSize = 12.000,
                        TextXAlignment = Enum.TextXAlignment.Left,
                        Text = isLabel,
                    })

                    function Labels:UpdateLabel(newLabel)
                        Text_Label.Text = newLabel
                    end

                    return Labels
                end

                function AddonMenu:AddKeybind(Info)
                    local Keybinds = {
                        Text = Info.Text,
                        Tooltip = Info.Tooltip,
                        Name = Info.Name,
                        Value = Info.Default,
                        Callback = Info.Callback or function(Value) end
                    }

                    local Keybind_Outer = Library:Create('Frame', {
                        Parent = Group_Inner,
                        BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                        BackgroundTransparency = 1.000,
                        BorderColor3 = Color3.fromRGB(0, 0, 0),
                        BorderSizePixel = 0,
                        Size = UDim2.new(1, 0, 0, 20),
                    })

                    local Keybind_Inner = Library:Create('Frame', {
                        Parent = Keybind_Outer,
                        BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                        BackgroundTransparency = 1.000,
                        BorderColor3 = Color3.fromRGB(0, 0, 0),
                        BorderSizePixel = 0,
                        Position = UDim2.new(0, 5, 0, 0),
                        Size = UDim2.new(1, -10, 1, 0),
                    })

                    local Keybind_Label = Library:Create('TextLabel', {
                        Parent = Keybind_Inner,
                        BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                        BackgroundTransparency = 1.000,
                        BorderColor3 = Color3.fromRGB(0, 0, 0),
                        BorderSizePixel = 0,
                        Position = UDim2.new(0, 160, 0, 0),
                        Size = UDim2.new(0, 50, 0, 20),
                        Font = Enum.Font.Ubuntu,
                        Text = Info.Name,
                        TextColor3 = Themes.Default.Item_Deactive,
                        TextSize = 10.000,
                        TextXAlignment = Enum.TextXAlignment.Right,
                    })

                    local Keybind_Title = Library:Create('TextLabel', {
                        Parent = Keybind_Inner,
                        BackgroundColor3 = Color3.fromRGB(255, 255, 255),
                        BackgroundTransparency = 1.000,
                        BorderColor3 = Color3.fromRGB(0, 0, 0),
                        BorderSizePixel = 0,
                        Size = UDim2.new(1, -50, 0, 20),
                        Font = Enum.Font.SourceSans,
                        TextColor3 = Themes.Default.Item_Deactive,
                        Text = Info.Text,
                        TextSize = 12.000,
                        TextXAlignment = Enum.TextXAlignment.Left,
                    })

                    local ActivatedKeybind

                    function Keybinds:UpdateKeybind()
                        ActivatedKeybind = Keybinds.Value
                        if not ActivatedKeybind then
                            Keybind_Title.TextColor3 = Themes.Default.Item_Active
                            Keybind_Label.TextColor3 = Themes.Default.Item_Active
                        else
                            Keybind_Title.TextColor3 = Themes.Default.Item_Deactive
                            Keybind_Label.TextColor3 = Themes.Default.Item_Deactive
                        end
                    end

                    UserInputService.InputBegan:Connect(function(input)
                        if input.KeyCode == Keybinds.Value then
                            Keybinds:UpdateKeybind()
                            Keybinds.Value = not Keybinds.Value
                            Keybinds.Callback(Keybinds.Value)
                        end
                    end)

                    if type(Info.Tooltip) == 'string' then
                        Library:AddToolTip(Info.Tooltip, Keybind_Title)
                    end

                    return Keybinds
                end

                return AddonMenu
            end
            return AddonGroup
        end
        return AddonSection
    end
    return AddonTab
end

return Libraryf
