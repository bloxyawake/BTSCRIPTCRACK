    local ServerTimeFunc = {};
    function ServerTimeFunc:Refresh(timeplay, timeserver)

        AccountStatus.Text = "Account Nickname    : "..DisplayName.."\nAccount User    : "..UserName.."\nAccount ID    : "..UserId.."\nAccount HWID    : "..HWID.."\nAccount Age    : "..GetAccountAge.." days\nAccount Time Left    : "..GetTimeExpired.."\nAccount Status    : "..GetStatus.."\nExploit    : "..GetExploit.." Executor\nServer Time    : "..timeserver.."\nYou have been playing for    : "..timeplay;

    end;

    function UpdateTime()
        local GameTime = math.floor(workspace.DistributedGameTime+0.5)
        local Hour = math.floor(GameTime/(60^2))%24
        local Minute = math.floor(GameTime/(60^1))%60
        local Second = math.floor(GameTime/(60^0))%60
        local TimePLaying = string.format("%02d.%02d.%02d", Hour, Minute, Second)

        local dt = DateTime.now()
        local Universaltime = dt:FormatUniversalTime("LLLL", "en-us")


        ServerTimeFunc:Refresh(TimePLaying, Universaltime)
    end
    game:GetService('RunService').Heartbeat:Connect(function()
        UpdateTime()
    end)

