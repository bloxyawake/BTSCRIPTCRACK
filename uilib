Toggle.BackgroundTransparency = 1.000
Toggle.BorderColor3 = Color3.fromRGB(0, 0, 0)
Toggle.BorderSizePixel = 0
Toggle.Position = UDim2.new(0.123021416, 0, 0.243134081, 0)
Toggle.Size = UDim2.new(0, 40, 0, 40)

UICorner.CornerRadius = UDim.new(0, 1000)
UICorner.Parent = Toggle

ToggleButton.Name = "ToggleButton"
ToggleButton.Parent = Toggle
ToggleButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
ToggleButton.BackgroundTransparency = 1.000
ToggleButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
ToggleButton.BorderSizePixel = 0
ToggleButton.Size = UDim2.new(1, 0, 1, 0)
ToggleButton.Image = "http://www.roblox.com/asset/?id=129267297340080"
ToggleButton.ImageColor3 = Color3.fromRGB(255, 0, 0)

function Library:HiddenUI(guiobject, guipartshow, guiparthide)
    ToggleButton.MouseButton1Click:Connect(function()
        if guiobject.Size == guipartshow and guiobject.Visible == true then

            Utility:Tween(guiobject, {Size = guiparthide}, .15)
            wait(.3)
            guiobject.Visible = false

        elseif guiobject.Size == guiparthide and guiobject.Visible == false then

            guiobject.Visible = true
            wait(.1)
            Utility:Tween(guiobject, {Size = guipartshow}, .15)

        end
    end)
end

Library:DragEnabled(ToggleButton);


function Library:CreateWindow(Version, Pictures)

    local Main_Body = Library:Create('ImageLabel', {
        Parent = BLACKTRAP,
        AnchorPoint = Vector2.new(0.5, 0.5),
        BackgroundColor3 = Color3.fromRGB(255, 255, 255),
        BackgroundTransparency = 1.000,
        BorderColor3 = Color3.fromRGB(0, 0, 0),
        BorderSizePixel = 0,
        ClipsDescendants = true,
        Visible = true,
        Position = UDim2.new(0.5, 0, 0.5, 0),
        Size = UDim2.new(0, 600, 0, 350),
        Image = Pictures,
    })

    Library:Create('UICorner', {
        CornerRadius = UDim.new(0, 8),
        Parent = Main_Body,
    })

    local isPart = {
        Show = UDim2.new(0, 600, 0, 350),
        Hide = UDim2.new(0, 0, 0, 0)
    }

    Library:AddStroke(Main_Body, Contextual, Brown)
    Library:HiddenUI(Main_Body, isPart.Show, isPart.Hide)

    Library:DragEnabled(Main_Body);

    Library:Create('ImageLabel', {
        Parent = Main_Body,
        BackgroundColor3 = Color3.fromRGB(255, 255, 255),
        BackgroundTransparency = 1.000,
        BorderColor3 = Color3.fromRGB(0, 0, 0),
        BorderSizePixel = 0,
        Position = UDim2.new(0, 10, 0, 5),
        Size = UDim2.new(0, 24, 0, 24),
