    function Library:AddBox(Info)
        local BoxSetting = {
            Text = Info.Text;
            Tooltip = Info.Tooltip;
            Value = Info.Default;
            Callback = Info.Callback or function(Value) end;
        };

        local Setting_Box = Library:Create('Frame', {
            Parent = Setting_Scroll,
            BackgroundColor3 = Color3.fromRGB(255, 255, 255),
            BackgroundTransparency = 1.000,
            BorderColor3 = Color3.fromRGB(0, 0, 0),
            BorderSizePixel = 0,
            Size = UDim2.new(1, 0, 0, 40),
        })

        local SettingBox_Inner = Library:Create('Frame', {
            Parent = Setting_Box,
            BackgroundColor3 = Themes.Default.Menus_Background,
            BorderColor3 = Color3.fromRGB(0, 0, 0),
            BorderSizePixel = 0,
            Position = UDim2.new(0, 5, 0, 5),
            Size = UDim2.new(1, -10, 0, 35),
        })

        Library:AddStroke(SettingBox_Inner, Border, Themes.Default.Lines)

        Library:Create('TextLabel', {
            Parent = SettingBox_Inner,
            BackgroundColor3 = Color3.fromRGB(255, 255, 255),
            BackgroundTransparency = 1.000,
            BorderColor3 = Color3.fromRGB(0, 0, 0),
            BorderSizePixel = 0,
            Position = UDim2.new(0, 10, 0, 0),
            Size = UDim2.new(0, 350, 1, 0),
            Font = Enum.Font.SourceSansBold,
            Text = Info.Text,
            TextColor3 = Themes.Default.Font,
            TextSize = 14.000,
            TextXAlignment = Enum.TextXAlignment.Left,
        })

        local TextBox = Library:Create('TextBox', {
            Parent = SettingBox_Inner,
            BackgroundColor3 = Themes.Default.Textbox_Background,
            BorderColor3 = Color3.fromRGB(0, 0, 0),
            BorderSizePixel = 0,
            Position = UDim2.new(0, 457, 0, 5),
            Size = UDim2.new(0, 100, 0, 25),
            ClearTextOnFocus = false,
            Font = Enum.Font.Arcade,
            Text = Info.Default,
            TextColor3 = Themes.Default.Textbox_Font,
            TextSize = 16.000,
        })

        Library:AddStroke(TextBox, Border, White)

        TextBox.FocusLost:Connect(function(EnterPressed)
            if not EnterPressed then 
                return;
            else
                Info.Callback(TextBox.Text);
                Library:AttemptSave();
            end;
        end);

        if type(Info.Tooltip) == 'string' then
            Library:AddToolTip(Info.Tooltip, SettingBox_Inner)
        end

        return BoxSetting
    end

    function Library:AddToggle(Info)
        local ToggleSetting = {
            Text = Info.Text;
            Tooltip = Info.Tooltip;
            Value = Info.Default or false;
            Callback = Info.Callback or function(Value) end;
        };

        local Setting_Toggle = Library:Create('Frame', {
            Parent = Setting_Scroll,
            BackgroundColor3 = Color3.fromRGB(255, 255, 255),
            BackgroundTransparency = 1.000,
            BorderColor3 = Color3.fromRGB(0, 0, 0),
            BorderSizePixel = 0,
            Size = UDim2.new(1, 0, 0, 40),
        })

        local Toggle_Inner = Library:Create('Frame', {
            Parent = Setting_Toggle,
            BackgroundColor3 = Themes.Default.Menus_Background,
            BorderColor3 = Color3.fromRGB(0, 0, 0),
            BorderSizePixel = 0,
            Position = UDim2.new(0, 5, 0, 5),
            Size = UDim2.new(1, -10, 0, 35),
        })

        Library:AddStroke(Toggle_Inner, Border, Themes.Default.Lines)

        Library:Create('TextLabel', {
            Parent = Toggle_Inner,
            BackgroundColor3 = Color3.fromRGB(255, 255, 255),
            BackgroundTransparency = 1.000,
            BorderColor3 = Color3.fromRGB(0, 0, 0),
            BorderSizePixel = 0,
            Position = UDim2.new(0, 10, 0, 0),
            Size = UDim2.new(0, 350, 1, 0),
            Font = Enum.Font.SourceSansBold,
            Text = Info.Text,
            TextColor3 = Themes.Default.Font,
            TextSize = 14.000,
            TextXAlignment = Enum.TextXAlignment.Left,
        })

        local TextButton = Library:Create('TextButton', {
            Parent = Toggle_Inner,
            BackgroundColor3 = Color3.fromRGB(255, 255, 255),
            BackgroundTransparency = 1.000,
            BorderColor3 = Color3.fromRGB(0, 0, 0),
            BorderSizePixel = 0,
            Position = UDim2.new(0, 482, 0, 0),
            Size = UDim2.new(0, 80, 1, 0),
            AutoButtonColor = false,
            Font = Enum.Font.SourceSans,
            Text = "",
            TextColor3 = Color3.fromRGB(0, 0, 0),
            TextSize = 1.000,
        })

        local Outer_Toggle = Library:Create('Frame', {
            Parent = TextButton,
            BackgroundColor3 = Color3.fromRGB(255, 0, 0),
            BorderColor3 = Color3.fromRGB(0, 0, 0),
            BorderSizePixel = 0,
            Position = UDim2.new(0, 17, 0, 5),
            Size = UDim2.new(0, 60, 0, 25),
        })

        Library:Create('UICorner', {
            CornerRadius = UDim.new(0, 100000),
            Parent = Outer_Toggle,
        })

        Library:AddStroke(Outer_Toggle, Border, White)

        local Inner_Toggle = Library:Create('Frame', {
            Parent = Outer_Toggle,
            BackgroundColor3 = Color3.fromRGB(220, 220, 220),
            BorderColor3 = Color3.fromRGB(0, 0, 0),
            BorderSizePixel = 0,
            Size = UDim2.new(0, 23, 0, 23),
            Position = UDim2.new(0, 1, 0, 1),
        })

        Library:Create('UICorner', {
            CornerRadius = UDim.new(0, 100000),
            Parent = Inner_Toggle,
        })

        local CheckToggle = false;
        if Info.Default then
            Utility:Tween(Outer_Toggle, {BackgroundColor3 = Color3.fromRGB(0, 255, 0)}, .15);
            Utility:Tween(Inner_Toggle, {Position = UDim2.new(0, 36, 0, 1)}, .15);
            CheckToggle = not CheckToggle;
            Info.Callback(CheckToggle);
        end;

        TextButton.MouseButton1Click:Connect(function()
            if not CheckToggle then
                Utility:Tween(Outer_Toggle, {BackgroundColor3 = Color3.fromRGB(0, 255, 0)}, .15);
                Utility:Tween(Inner_Toggle, {Position = UDim2.new(0, 36, 0, 1)}, .15);
                Library:AttemptSave();
            else
                Utility:Tween(Outer_Toggle, {BackgroundColor3 = Color3.fromRGB(255, 0, 0)}, .15);
                Utility:Tween(Inner_Toggle, {Position = UDim2.new(0, 1, 0, 1)}, .15);
            end;

            CheckToggle = not CheckToggle;
            Info.Callback(CheckToggle);
        end);

        if type(Info.Tooltip) == 'string' then
            Library:AddToolTip(Info.Tooltip, Toggle_Inner);
        end;

        return ToggleSetting
    end

    function Library:AddButton(Info)

        local ToggleButton = {
            Text = Info.Text;
            Tooltip = Info.Tooltip;
            Callback = Info.Callback or function() end;
        };

        local SettingButton = Library:Create('Frame', {
            Parent = Setting_Scroll,
            BackgroundColor3 = Color3.fromRGB(255, 255, 255),
            BackgroundTransparency = 1.000,
            BorderColor3 = Color3.fromRGB(0, 0, 0),
            BorderSizePixel = 0,
            Size = UDim2.new(1, 0, 0, 40),
        })

        local SettingButtonInner = Library:Create('Frame', {
            Parent = SettingButton,
            BackgroundColor3 = Themes.Default.Menus_Background,
            BorderColor3 = Color3.fromRGB(0, 0, 0),
            BorderSizePixel = 0,
            Position = UDim2.new(0, 5, 0, 5),
            Size = UDim2.new(1, -10, 0, 35),
        })

        Library:AddStroke(SettingButtonInner, Border, Themes.Default.Lines)

        local isTitle = Library:Create('TextLabel', {
            Parent = SettingButtonInner,
            BackgroundColor3 = Color3.fromRGB(255, 255, 255),
            BackgroundTransparency = 1.000,
            BorderColor3 = Color3.fromRGB(0, 0, 0),
            BorderSizePixel = 0,
            Position = UDim2.new(0, 10, 0, 0),
            Size = UDim2.new(0, 350, 1, 0),
            Font = Enum.Font.SourceSansBold,
            Text = Info.Text,
            TextColor3 = Themes.Default.Font,
            TextSize = 14.000,
            TextXAlignment = Enum.TextXAlignment.Left,
        })

        local FrameButton = Library:Create('Frame', {
            Parent = SettingButtonInner,
            BackgroundColor3 = Themes.Default.Setting_Button_Background_Active,
            AnchorPoint = Vector2.new(0.5, 0.5),
            BorderColor3 = Color3.fromRGB(0, 0, 0),
            BorderSizePixel = 0,
            Position = UDim2.new(0, 498, 0, 17),
            Size = UDim2.new(0, 120, 0, 25),
        })

        local Button = Library:Create('TextButton', {
            Parent = SettingButtonInner,
            BackgroundColor3 = Color3.fromRGB(255, 255, 255),
            BackgroundTransparency = 1.000,
            BorderColor3 = Color3.fromRGB(0, 0, 0),
            BorderSizePixel = 0,
            Position = UDim2.new(0, 439, 0, 0),
            Size = UDim2.new(0, 120, 0, 35),
            AutoButtonColor = false,
            Font = Enum.Font.SourceSansBold,
            Text = "CLICK HERE",
            TextColor3 = Themes.Default.Setting_Button_Font_Active,
            TextSize = 14.000,
        })

        Library:AddStroke(FrameButton, Border, Themes.Default.Lines)

        Button.MouseEnter:Connect(function()
            Utility:Tween(FrameButton, {BackgroundColor3 = Themes.Default.Setting_Button_Background_Deactive}, .15)
            Utility:Tween(FrameButton, {Size = UDim2.new(0, 110, 0, 25)}, .15)
        end)

        Button.MouseLeave:Connect(function()
            Utility:Tween(FrameButton, {BackgroundColor3 = Themes.Default.Setting_Button_Background_Active}, .15)
            Utility:Tween(FrameButton, {Size = UDim2.new(0, 120, 0, 25)}, .15)
        end)

        Button.MouseButton1Down:Connect(function()
            Utility:Tween(FrameButton, {BackgroundColor3 = Themes.Default.Setting_Button_Background_Active}, .3)
            Utility:Tween(FrameButton, {Size = UDim2.new(0, 120, 0, 25)}, .3)
        end)

        Button.MouseButton1Up:Connect(function()
            Utility:Tween(FrameButton, {BackgroundColor3 = Themes.Default.Setting_Button_Background_Deactive}, .3)
            Utility:Tween(FrameButton, {Size = UDim2.new(0, 110, 0, 25)}, .3)
        end)

        Button.MouseButton1Click:Connect(function()
            Button.Text = "."
            wait(.3)
            Button.Text = ".."
            wait(.3)
            Button.Text = "..."
            wait(.3)
            Button.Text = "CLICK HERE"
            Info.Callback();
        end)

        if type(Info.Tooltip) == 'string' then
            Library:AddToolTip(Info.Tooltip, isTitle);
        end;
        return ToggleButton;

    end

    getgenv().Jump = 50
    Library:AddBox({
        Text = 'Jump Hack',
        Tooltip = 'Set the value to make your jump high.',
        Default = getgenv().Jump,
        Callback = function(Value)
            getgenv().Jump = Value

            if getgenv().Jump then
                game:GetService("Players").LocalPlayer.Character.Humanoid.JumpPower = getgenv().Jump
            end
        end
    })

    getgenv().SpeedHack = 26
    Library:AddBox({
        Text = 'Speed Hack',
        Tooltip = 'Set the value to make your speed fast.',
        Default = getgenv().SpeedHack,
        Callback = function(Value)
            getgenv().SpeedHack = Value

            if getgenv().SpeedHack then
                Player.Character.Humanoid.WalkSpeed = getgenv().SpeedHack
                Player.Character.Humanoid:GetPropertyChangedSignal('WalkSpeed'):Connect(function()
                    Player.Character.Humanoid.WalkSpeed = getgenv().SpeedHack
                end)
            end
        end
    })
    
    local Flying = false

    function toggleFly()
        local Camera = workspace.CurrentCamera
        Flying = not Flying
        
        if Flying then
            local char = Player.Character
            local rootPart:BasePart = char:WaitForChild('HumanoidRootPart')
            local hum:Humanoid = char:WaitForChild('Humanoid')
            hum.PlatformStand = true
            local currentCF = rootPart.CFrame
    
            while Flying and task.wait() do
                local add = Vector3.new(0,0,0)
    
                if UserInputService:IsKeyDown(Enum.KeyCode.W) then add += Camera.CFrame.LookVector end
                if UserInputService:IsKeyDown(Enum.KeyCode.S) then add -= Camera.CFrame.LookVector end
                if UserInputService:IsKeyDown(Enum.KeyCode.D) then add += Camera.CFrame.RightVector end
                if UserInputService:IsKeyDown(Enum.KeyCode.A) then add -= Camera.CFrame.RightVector end
                if UserInputService:IsKeyDown(Enum.KeyCode.E) then add += Camera.CFrame.UpVector end
                if UserInputService:IsKeyDown(Enum.KeyCode.Q) then add -= Camera.CFrame.UpVector end
    
                rootPart.AssemblyLinearVelocity = Vector3.zero
                rootPart.AssemblyAngularVelocity = Vector3.zero
    
                currentCF += add
                rootPart.CFrame = CFrame.lookAt(
                    currentCF.Position,
                    currentCF.Position + (Camera.CFrame.LookVector * 2)
                )
            end
    
            hum.PlatformStand = false
        end
    end

    Library:AddToggle({
        Text = 'Flying Hack',
        Tooltip = 'Use W,A,S,D and camera for run fly',
        AutoFly = false,
        Callback = function(Value)
            AutoFly = Value
            
            if AutoFly then
                toggleFly()
            else
                Flying = false
            end
        end
    })

    AntiKick = true
    Library:AddToggle({
        Text = 'Anti Kick',
        Tooltip = 'Make you cannot get kick when AFK.',
        Default = AntiKick,
        Callback = function(Value)
            AntiKick = Value
            
            if AntiKick then
                Player.Idled:connect(function()
                    game:GetService("VirtualUser"):Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
                    wait(.1)
                    game:GetService("VirtualUser"):Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
                end)
            end
        end
    })

    Library:AddButton({
        Text = 'Infine Jump', 
        Tooltip = 'Make you infine jump.',
        Callback = function()
            UserInputService.JumpRequest:Connect(function()
                Player.Character:FindFirstChildOfClass('Humanoid'):ChangeState("Jumping")
            end)
        end
    })

    Library:AddButton({
        Text = 'No Clips', 
        Tooltip = 'Make you no clip.',
        Callback = function()
            game:GetService("RunService").Stepped:Connect(function()
                for i,v in pairs(game:GetService("Players").LocalPlayer.Character:GetDescendants()) do
                    if v:IsA("BasePart") then
                        v.CanCollide = false
                    end
                end
            end)
        end
    })

    Library:AddButton({
        Text = 'FPS Boost', 
        Tooltip = 'Make you game graphich too low.',
        Callback = function()
            local a = tick()
            if not game:IsLoaded() then
                game.Loaded:Wait()
            end
            wait(.1)
            local g = game
            local w = g.Workspace
            local l = g.Lighting
            local t = w.Terrain
            t.WaterWaveSize = 0
            t.WaterWaveSpeed = 0
            t.WaterReflectance = 0
            t.WaterTransparency = 0
            l.GlobalShadows = false
            l.FogEnd = 9e9
            l.Brightness = 0
            settings().Rendering.QualityLevel = "Level01"
            for i, v in pairs(g:GetDescendants()) do
                if v:IsA("Part") or v:IsA("Union") or v:IsA("CornerWedgePart") or v:IsA("TrussPart") then 
                    v.Material = Enum.Material.Plastic
                    v.Reflectance = 0
                elseif v:IsA("Decal") or v:IsA("Texture") then
                    v.Transparency = 1
                elseif v:IsA("ParticleEmitter") or v:IsA("Trail") then
                    v.Lifetime = NumberRange.new(0)
                elseif v:IsA("Explosion") then
                    v.BlastPressure = 1
                    v.BlastRadius = 1
                elseif v:IsA("Fire") or v:IsA("SpotLight") or v:IsA("Smoke") or v:IsA("Sparkles") then
                    v.Enabled = false
                elseif v:IsA("MeshPart") then
                    v.Material = Enum.Material.Plastic
                    v.Reflectance = 0
                    v.TextureID = 10385902758728957
                elseif v:IsA("BasePart") then
                    v.Reflectance = 0
                    v.CastShadow = false
                end
            end
            for i, e in pairs(l:GetChildren()) do
                if e:IsA("BlurEffect") or e:IsA("SunRaysEffect") or e:IsA("ColorCorrectionEffect") or e:IsA("BloomEffect") or e:IsA("DepthOfFieldEffect") or e:IsA("PostEffect") then
                    e.Enabled = false
                end
            end
            warn("Low graphics loaded! (" .. math.floor(tick() - a) .. "s)")

        end
    })

    Library:AddButton({
        Text = 'Rejoin Server', 
        Tooltip = 'Rejoin to the same server.',
